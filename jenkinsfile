pipeline {
    agent any

    stages {
        stage('Source') {
            steps {
                git branch: 'main', credentialsId: 'git hub credentials', url: 'https://github.com/vinaypolineni/jenkinstestprivate.git'
            }
        }
    
        stage('Build') {
            steps {
                sh 'ls -la'
            }
        }
    
        stage('Stage 3') {
            steps {
                echo 'Hello World'
            }
        }
    }    
}
